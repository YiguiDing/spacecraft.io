{
  "name": "server",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node ./src/example.ts",
    "build": "npm run build:index && npm run build:types",
    "build:index": "tsc --build --project tsconfig.json",
    "build:types": "tsc-alias -p tsconfig.json",
    "run": "node ./dist/index.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  }
}
