    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.1",
    "@types/stats.js": "^0.17.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "typescript": "^5.2.2",
